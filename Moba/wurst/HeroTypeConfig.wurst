        ..setMax(Stat.AD, 100)
        ..setMax(Stat.AR, 100)
        ..setMax(Stat.AS, 100)
        ..setMax(Stat.HP, 100)
        ..setMax(Stat.HR, 100)
        ..setMax(Stat.MP, 100)
        ..setMax(Stat.MR, 100)
        ..setMax(Stat.MS, 100)




init
